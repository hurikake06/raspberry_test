<!DOCTYPE html>
<html>
  <head>
    <title>RaspberryTest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <style media="screen">
    </style>
  </head>

  <body>
    <header>
      <%= link_to 'RassberryTest', root_path %>
